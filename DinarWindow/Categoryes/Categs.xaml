<Window x:Class="DinarWindow.Categs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DinarWindow"
        mc:Ignorable="d"
        Title="Categs" Height="197" Width="260">
    <Grid x:Name="Window2" Margin="0,0,0,-6">
        <Button x:Name="ButtonAddCategory" Content="Добавить" HorizontalAlignment="Left" Height="61" Margin="23,116,0,0" VerticalAlignment="Top" Width="190" Click="ButtonAddCategory_Click"/>
        <TextBox x:Name="TextBoxSum" HorizontalAlignment="Left" Margin="23,27,0,0" Text="Сумма" TextWrapping="Wrap" VerticalAlignment="Top" Width="68" Height="49" ContextMenuOpening="TextBox_ContextMenuOpening"/>
        <TextBox x:Name="TextBoxCategoryName" HorizontalAlignment="Left" Margin="96,27,0,0" Text="Название категории" TextWrapping="Wrap" VerticalAlignment="Top" Width="117" Height="49"/>
        <DatePicker x:Name="DatePickerTranzDay" HorizontalAlignment="Left" Margin="23,0,0,0" VerticalAlignment="Center" Width="190"/>

    </Grid>
</Window>
